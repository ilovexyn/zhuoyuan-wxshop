<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhuoyuan.wxshop.mapper.StoreHouseRecordsMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhuoyuan.wxshop.model.StoreHouseRecords">
        <id column="id" property="id" />
        <result column="goods_id" property="goodsId" />
        <result column="order_id" property="orderId" />
        <result column="store_id" property="storeId" />
        <result column="count" property="count" />
        <result column="ct" property="ct" />
        <result column="ut" property="ut" />
        <result column="ordercode" property="ordercode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, goods_id, order_id, store_id, count, ct, ut, ordercode
    </sql>

</mapper>
